                                                    "example": "admin"
                                                },
                                                "updated_at": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2025-05-10T13:19:55.609000Z"
                                                },
                                                "created_at": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2025-05-10T13:19:55.609000Z"
                                                },
                                                "id": {
                                                    "type": "string",
                                                    "example": "681f527b8ab7526eb80a1d12"
